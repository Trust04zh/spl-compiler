// modified from https://www.luogu.com.cn/problem/solution/P1002

int fx[9];
int fy[9];

int bx, by, mx, my;
int f[40][40];
int s[40][40];
int main(){
    int i, j;
    bx = bx + 2; 
    by = by + 2; 
    mx = mx + 2; 
    my = my + 2;
    f[2][1] = 1;
    s[mx][my] = 1;
    while (i <= 8) {
        i = i + 1;
        s[mx + fx[i]][my + fy[i]] = 1;
    }
    for(i = 2; i <= bx; i = i + 1){
        for(j = 2; j <= by; j = j + 1){
            if(s[i][j]) continue;
            f[i][j] = f[i - 1][j] + f[i][j - 1];
        }
    }
    return 0;
} 